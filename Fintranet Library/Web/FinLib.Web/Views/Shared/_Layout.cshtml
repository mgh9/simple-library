<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="Description" content="Fintranet Library">

    <title>@ViewBag.Title</title>

    <link rel="icon" type="image/x-icon" href="~/favicon.ico" />
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />

    <link rel="preload" as="font" crossorigin="anonymous" href="/fonts/font-awesome/webfonts/fa-solid-900.woff2">

    <link href="~/css/site-bundle.min.css" rel="stylesheet" asp-append-version="true" />

    @RenderSection("styles", required: false)
</head>
<body>
    <main class="bg">
        @RenderBody()
    </main>

    <script src="~/js/site-bundle.min.js" type="text/javascript" asp-append-version="true"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
